{
  "name": "imagepetapeta-beta",
  "productName": "ImagePetaPeta-beta",
  "version": "1.9.4-beta",
  "private": true,
  "author": "takumus",
  "description": "image viewer",
  "scripts": {
    "serve": "vue-cli-service electron:serve",
    "build": "vue-cli-service electron:build",
    "lint": "vue-cli-service lint",
    "linecount": "node ./scripts/linecount",
    "generate-licenses": "license-checker --production --json > ./licenses.json && node ./scripts/generateLicenses.js",
    "generate-icons": "node ./scripts/generateIcons.js",
    "generate-cursors": "node ./scripts/generateCursors.js",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "preserve": "yarn generate-assets",
    "prebuild": "rimraf ./dist_electron && yarn generate-assets",
    "generate-assets": "yarn generate-licenses && yarn generate-icons && yarn generate-cursors",
    "release": "yarn generate-licenses && node ./scripts/release.js && yarn build"
  },
  "main": "background.js",
  "dependencies": {
    "@araki-packages/promise-serial": "^0.2.1",
    "@pixi/gif": "^1.0.2",
    "@seald-io/nedb": "^2.2.1",
    "@types/fuzzy-search": "^2.1.2",
    "axios": "^0.21.4",
    "blurhash": "^1.1.4",
    "core-js": "^3.6.5",
    "data-uri-to-buffer": "^3.0.1",
    "dateformat": "^5.0.3",
    "deepcopy": "^2.1.0",
    "fuzzy-search": "^3.2.1",
    "pixi.js": "^6.1.3",
    "sharp": "^0.29.0",
    "uuid": "^8.3.2",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-i18n": "^9.2.0-beta.6",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@shockpkg/icon-encoder": "^1.3.1",
    "@types/dateformat": "^5.0.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/mime-types": "^2.1.1",
    "@types/nedb": "^1.8.12",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/sharp": "^0.28.6",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "electron": "^17.0.1",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "license-checker": "^25.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5",
    "vue-cli-plugin-electron-builder": "~2.1.1"
  },
  "volta": {
    "node": "16.13.2",
    "yarn": "1.22.17"
  }
}
