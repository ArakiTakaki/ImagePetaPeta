    "nedb": "^1.8.0",
    "sharp": "^0.29.0",
    "uuid": "^8.3.2",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
